{
  "name": "users",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "string",
      "required": true
    },
    "isStoreOwner": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "model": "AccessToken",
      "type": "hasMany",
      "foreignKey": "userId"
    },
    "stores": {
      "type": "belongsTo",
      "model": "stores",
      "foreignKey": "storeId"
    }
  },
  "acls": [],
  "methods": {}
}
